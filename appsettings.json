{
  "ConnectionStrings": {
    "StorageAccount": "<storage_account_key>"
  },
  "Sessionize": {
    "ApiUrl": "https://sessionize.com/api/v2/{{EventId}}/view",
    "EventId": "<event_id>"
  },
  "Serilog": {
    "SeqServerUrl": null,
    "LogstashUrl": null,
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.eShopOnContainers": "Information",
        "System": "Warning"
      }
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": "<app_insights_key>"
  }
}